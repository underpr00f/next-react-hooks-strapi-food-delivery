{
  "kind": "collectionType",
  "collectionName": "donates",
  "info": {
    "name": "Donate"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "message": {
      "type": "string",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "donates"
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "anothermessage": {
      "type": "string"
    },
    "amount": {
      "type": "biginteger"
    },
    "order_id": {
      "type": "biginteger"
    }
  }
}
